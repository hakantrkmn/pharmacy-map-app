{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/preload-pharmacies",
      "schedule": "0 8 * * *"
    }
  ],
  "env": {
    "REDIS_URL": "@redis_url"
  }
}
